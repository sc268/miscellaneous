    "def plotter(breaks, window_l, window_r):\n",
    "    # plot segmentations\n",
    "    for break_pt in breaks:\n",
    "        if break_pt >= window_l and break_pt <= window_r:\n",
    "            plt.axvline(break_pt, color = 'lightblue')"
   ]
