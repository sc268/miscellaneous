    "# DEMO\n",
    "\n",
    "# plot segements\n",
    "def plot_segments(breaks, window_l, window_r):\n",
    "    for i in range(len(breaks)):\n",
    "        if breaks[i] in range(window_l, window_r):\n",
    "            plt.axvline(breaks[i], color = 'lightblue')\n",
    "            \n",
    "def step_function_fitting(x, y):\n",
    "    X = np.array(x).reshape(len(x), 1)\n",
    "    st = DecisionTreeRegressor(max_depth=2).fit(X, y).predict(X)\n",
    "    ip = list(np.sort(np.where(np.diff(st) > 0)[0])) \n",
    "    ip = np.array([1] + ip + [len(x)])-1\n",
    "    return ip\n",
    "\n",
    "# greedy approach module\n",
    "# input curve x, curve y, num of partition\n",
    "# output best partition breaks = [array]\n",
    "from scipy.interpolate import interp1d\n",
    "\n",
    "def greedy_fitting_old(x, y, num_partitions, loss_fcn_power = 2):\n",
    "    breaks = [0, 999]\n",
    "    current_partitions = 1\n",
    "    while (current_partitions < num_partitions):\n",
    "        score_table = {}\n",
    "        for i in list(set(range(0, 999))-set(breaks)):\n",
    "            tmp_breaks = breaks[:]\n",
    "            tmp_breaks.append(i)\n",
    "            tmp_breaks.sort()\n",
    "            piecewise_approx = interp1d(x[tmp_breaks], y[tmp_breaks])\n",
    "            loss = sum(abs(piecewise_approx(x)-y)**loss_fcn_power)\n",
    "            score_table[loss] = tmp_breaks\n",
    "        breaks = score_table[min(score_table.keys())]\n",
    "        current_partitions += 1\n",
    "    return breaks\n",
    "\n",
    "def greedy_fitting(x, y, num_partitions, loss_fcn_power = 1):\n",
    "    breaks = [0, 999]\n",
    "    current_partitions = 1\n",
    "    while (current_partitions < num_partitions):\n",
    "        score_table = {}\n",
    "        for i in list(set(range(0, 999))-set(breaks)):\n",
    "            tmp_breaks = breaks[:]\n",
    "            tmp_breaks.append(i)\n",
    "            tmp_breaks.sort()\n",
    "            \n",
    "            # fit several piecewise lines\n",
    "            full_breaks = list(set(tmp_breaks + [i-1 for i in tmp_breaks if i not in [0,999]]))\n",
    "            piecewise_approx = interp1d(x[full_breaks], y[full_breaks])\n",
    "            loss = sum(abs(piecewise_approx(x)-y)**loss_fcn_power)\n",
    "            score_table[loss] = tmp_breaks\n",
    "        breaks = score_table[min(score_table.keys())]\n",
    "        \n",
    "        # early stops if nearly perfect fitting\n",
    "        if min(score_table.keys()) < 0.01:\n",
    "            break\n",
    "        current_partitions += 1\n",
    "    \n",
    "    if len(breaks) == num_partitions + 1:\n",
    "        print breaks, len(breaks), num_partitions+1\n",
    "        return breaks\n",
    "    else:\n",
    "        res = cluster.KMeans(n_clusters= num_partitions).fit_predict(y.reshape(len(y),1))\n",
    "        breaks = list(np.where(np.diff(res) != 0)[0])\n",
    "        breaks = [0] + breaks + [len(res)-1]\n",
    "        return breaks\n",
    "\n",
    "\n",
    "# K-means on predictions module\n",
    "from sklearn import cluster\n",
    "def kmeans_preds(y, num_partitions):\n",
    "    res = cluster.KMeans(n_clusters= num_partitions).fit_predict(y.reshape(len(y),1))\n",
    "    breaks = list(np.where(np.diff(res) != 0)[0])\n",
    "    breaks = [0] + breaks + [len(res)-1]\n",
    "    return breaks\n",
    "\n",
    "\n",
    "# all methods\n",
    "def all_fitting(method, x, y_input, num_partitions):\n",
    "    if method=='step_function':\n",
    "        return step_function_fitting(x, y_input)\n",
    "    elif method =='kmeans':\n",
    "        return kmeans_preds(y_input, num_partitions)\n",
    "    elif method =='greedy_abs':\n",
